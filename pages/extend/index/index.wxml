
<view class="extend-box padding flex flex-wrap justify-between">
  <navigator wx:for="{{list}}" wx:key class="extendItem free-style-list bg-{{item.color}} padding radius-sm margin-bottom justify-between" url="/pages/extend/{{item.title}}/{{item.title}}">
    <view class="flex align-center justify-between" >
      <text class="text-lg ">{{item.name}}</text>
    </view>
    <view class="extend-style-text">{{item.title}}
    </view>
  </navigator>
</view> 