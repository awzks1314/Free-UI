<view class="free-tabbar tabbar {{isFixed?'free-fixed':''}}" style="background:{{backgroundColor}};">
  <view wx:for="{{list}}" wx:key="index" class="free-tabbar-item {{item.center?'free-action':''}}" catchtap="handleClick" data-index="{{index}}" data-url="{{item.url}}">
      <free-image src="{{current == index ?item.selectIcon:item.icon}}" width="{{item.center?'70rpx':'40rpx'}}"
    height="{{item.center?'70rpx':'40rpx'}}" radius="50%" class="free-icon" ></free-image>
      <view class="free-tabbar-tag" wx:if="{{item.number>0}}">{{item.number}}</view>
      <text style="color:{{current == index ?selectColor:color}};">{{item.title}}</text>
  </view>
</view>
