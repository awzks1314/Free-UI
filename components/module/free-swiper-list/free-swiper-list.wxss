.free-swipeout-wrap {
  background: #fff;
  position: relative;
  overflow: hidden;
}
.swipe-action-show{
  position: relative;
  z-index: 99999
}
.free-swipeout-item {
  width: 100%;
  /* padding: 15px 20px; */
  box-sizing: border-box;
  transition: transform 0.2s ease;
  font-size: 14px;
}

.free-swipeout-content {
  white-space: nowrap;
  overflow: hidden;
}

.free-swipeout-button-right-group {
  position: absolute;
  right: -100%;
  top: 0;
  height: 100%;
  z-index: 1;
  width: 100%;
}

.free-swipeout-button-right-item {
  height: 100%;
  float: left;
  white-space: nowrap;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
}
	.swipe-action_mask {
		display: block;
		opacity: 0;
		position: fixed;
		z-index: 999;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
	}
