/* ==================
          按钮
 ==================== */
	
 .free-button {
	position: relative;
	border: 0rpx;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	box-sizing: border-box;
	padding: 0 30rpx;
	font-size: 28rpx;
	height: 60rpx;
	line-height: 1;
	text-align: center;
	text-decoration: none;
	overflow: visible;
	margin-left: initial;
	transform: translate(0rpx, 0rpx);
	margin-right: initial;
}

.free-button::after {
	display: none;
}

.free-button:not([class*="bg-"]) {
	background-color: #f0f0f0;
}

.free-button[class*="line"] {
	background-color: transparent;
}

.free-button[class*="line"]::after {
	content: " ";
	display: block;
	width: 200%;
	height: 200%;
	position: absolute;
	top: 0;
	left: 0;
	border: 1rpx solid currentColor;
	transform: scale(0.5);
	transform-origin: 0 0;
	box-sizing: border-box;
	border-radius: 12rpx;
	z-index: 1;
	pointer-events: none;
}

.free-button.round[class*="line"]::after {
	border-radius: 1000rpx;
}

.free-button[class*="lines"]::after {
	border: 6rpx solid currentColor;
}

.free-button[class*="bg-"]::after {
	display: none;
}

.free-button.sm {
	padding: 0 20rpx;
	font-size: 20rpx;
	height: 50rpx;
}

.free-button.lg {
	padding: 0 40rpx;
	font-size: 32rpx;
	height: 70rpx;
}

.free-button.icon.sm {
	width: 50rpx;
	height: 50rpx;
}

.free-button.icon {
	width: 64rpx;
	height: 64rpx;
	border-radius: 500rpx;
	padding: 0;
}

button.icon.lg {
	width: 70rpx;
	height: 70rpx;
}

.free-button.shadow-blur::before {
	top: 4rpx;
	left: 4rpx;
	filter: blur(6rpx);
	opacity: 0.6;
}

.free-button.button-hover {
	transform: translate(1rpx, 1rpx);
}

.block {
	display: block;
}

.free-button.block {
	display: flex;
}

.free-button[disabled] {
	opacity: 0.6;
	color: var(--white);
}
/* ==================
         表单
 ==================== */

 .free-form-group {
	background-color: var(--white);
	padding: 1rpx 30rpx;
	display: flex;
	align-items: center;
	min-height: 100rpx;
	justify-content: space-between;
}

.free-form-group+.free-form-group {
	border-top: 1rpx solid #eee;
}

.free-form-group .title {
	text-align: justify;
	padding-right: 30rpx;
	font-size: 30rpx;
	position: relative;
	height: 60rpx;
	line-height: 60rpx;
}

.free-form-group input {
	flex: 1;
	font-size: 30rpx;
	color: #555;
	padding-right: 20rpx;
}

.free-form-group>text[class*="cuIcon-"] {
	font-size: 36rpx;
	padding: 0;
	box-sizing: border-box;
}

.free-form-group textarea {
	margin: 32rpx 0 30rpx;
	height: 4.6em;
	width: 100%;
	line-height: 1.2em;
	flex: 1;
	font-size: 28rpx;
	padding: 0;
}

.free-form-group.align-start .title {
	height: 1em;
	margin-top: 32rpx;
	line-height: 1em;
}

.free-form-group picker {
	flex: 1;
	padding-right: 40rpx;
	overflow: hidden;
	position: relative;
}

.free-form-group picker .picker {
	line-height: 100rpx;
	font-size: 28rpx;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	width: 100%;
	text-align: right;
}

.free-form-group picker::after {
	font-family: "cuIcon";
	display: block;
	content: "\e6a3";
	position: absolute;
	font-size: 34rpx;
	color: var(--grey);
	line-height: 100rpx;
	width: 60rpx;
	text-align: center;
	top: 0;
	bottom: 0;
	right: -20rpx;
	margin: auto;
}

.free-form-group textarea[disabled],
.free-form-group textarea[disabled] .placeholder {
	color: transparent;
}

/* ==================
         模态窗口
 ==================== */

 .free-modal {
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 1110;
	opacity: 0;
	outline: 0;
	text-align: center;
	-ms-transform: scale(1.185);
	transform: scale(1.185);
	backface-visibility: hidden;
	perspective: 2000rpx;
	background: rgba(0, 0, 0, 0.6);
	transition: all 0.3s ease-in-out 0s;
	pointer-events: none;
}

.free-modal::before {
	content: "\200B";
	display: inline-block;
	height: 100%;
	vertical-align: middle;
}

.free-modal.show {
	opacity: 1;
	transition-duration: 0.3s;
	-ms-transform: scale(1);
	transform: scale(1);
	overflow-x: hidden;
	overflow-y: auto;
	pointer-events: auto;
}

.free-dialog {
	position: relative;
	display: inline-block;
	vertical-align: middle;
	margin-left: auto;
	margin-right: auto;
	width: 680rpx;
	max-width: 100%;
	background-color: #f8f8f8;
	border-radius: 10rpx;
	overflow: hidden;
}

.free-modal.bottom-modal::before {
	vertical-align: bottom;
}

.free-modal.bottom-modal .free-dialog {
	width: 100%;
	border-radius: 0;
}

.free-modal.bottom-modal {
	margin-bottom: -1000rpx;
}

.free-modal.bottom-modal.show {
	margin-bottom: 0;
}

.free-modal.drawer-modal {
	transform: scale(1);
	display: flex;
}

.free-modal.drawer-modal .free-dialog {
	height: 100%;
	min-width: 200rpx;
	border-radius: 0;
	margin: initial;
	transition-duration: 0.3s;
}

.free-modal.drawer-modal.justify-start .free-dialog {
	transform: translateX(-100%);
}

.free-modal.drawer-modal.justify-end .free-dialog {
	transform: translateX(100%);
}

.free-modal.drawer-modal.show .free-dialog {
	transform: translateX(0%);
}
.free-modal .free-dialog>.free-bar:first-child .action{
  min-width: 100rpx;
  margin-right: 0;
  min-height: 100rpx;
}
.free-bar .content {
	position: absolute;
	text-align: center;
	width: calc(100% - 340rpx);
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	height: 60rpx;
	font-size: 32rpx;
	line-height: 60rpx;
	cursor: none;
	pointer-events: none;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}


